  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Custom Select</title>
  <link rel="stylesheet" href="select.css">
  <script src="./main.js" defer type="module"></script>
</head>

<body>
  <select type="select" data-custom>
